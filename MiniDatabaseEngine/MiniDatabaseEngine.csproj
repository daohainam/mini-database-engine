<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Embedded database engine for education</Title>
	<Version>0.1.0</Version>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	<PackageProjectUrl>https://github.com/daohainam/mini-database-engine</PackageProjectUrl>
	<RepositoryUrl>https://github.com/daohainam/mini-database-engine</RepositoryUrl>
</PropertyGroup>

</Project>
